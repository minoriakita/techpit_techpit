<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.akita.techpit.chat.chatbackend.infrastructure.messages.MessageMapper">
  <insert id="insert" parameterType="com.akita.techpit.chat.chatbackend.domain.messages.model.Message">
    INSERT INTO messages
      (id, channel_id, text, username, timestamp)
    VALUES
      (#{id}, #{channelId}, #{text}, #{username}, #{timestamp})
  </insert>
</mapper>